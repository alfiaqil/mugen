; Jump Up
[Statedef 50]
type    = A
physics = A

;[State 50, 1]
;type = VarSet
;trigger1 = Time = 0
;sysvar(1) = 0

[State 50, From_Pre-Jump]
type = ChangeAnim
triggerall = !(Anim = [41,59])
triggerall = (PrevStateNo = [39,45])
trigger1 = Time = 0
value = ifelse((vel x)=0, 41, ifelse((vel x)>0, 42, 43))

[State 50, Not_From_Pre-Jump]
type = ChangeAnim
triggerall = !(Anim = [41,59])
triggerall = !(PrevStateNo = [39,45])
trigger1 = Time = 0
value = 42
Elem = 5

[State 50, AirJump_Dust]
type = Helper
triggerall = Time = 0
trigger1 = PrevStateNo = 260 || PrevStateNo = 1050
trigger2 = var(9) = 3 && PrevStateNo < 50
helpertype = Normal
name = "AirJump_Dust"
ID = 7017
StateNo = 7017
postype = p1
pos = 0 * Const(Size.xScale), Const(Size.mid.pos.y) * Const(Size.yScale)
facing = 1
keyctrl = 0
ownpal = 1
;remappal = 1,1
supermovetime = 4294967295
pausemovetime = 4294967295
size.xscale = Const(Size.xScale) * 1
size.yscale = Const(Size.yScale) * 1
;size.proj.doscale = 0
size.shadowoffset = -30 * sysvar(1);Degree
ignorehitpause = 1

[State 50, SND-FX]
type = PlaySnd
triggerall = PrevStateNo = 260 || PrevStateNo = 1050
trigger1 = Time = 0
value = 0,102
channel = 1

[State 50, 3]
type = ChangeAnim
triggerall = (PrevStateNo = [39,45])
trigger1 = Vel y > Const720p(-8)
trigger1 = SelfAnimExist(anim + 3)
trigger1 = anim >= 41 && anim <= 43
persistent = 0
value = Anim + 3

[State 50, Jump-Dust]
type = Helper
triggerall = PrevStateNo = [39,40]
trigger1 = !time
helpertype = Normal
name = "Jump-Dust"
ID = 7042
StateNo = 7042
postype = p1
pos = 0 * Const(Size.xScale), 0 * Const(Size.yScale)
facing = 1
keyctrl = 0
ownpal = 1
;remappal = 1,1
;supermovetime = 4294967295
;pausemovetime = 4294967295
size.xscale = Const(Size.xScale) * 1
size.yscale = Const(Size.yScale) * 1
size.head.pos = Floor(vel x), Floor(vel y)
;size.proj.doscale = 1
ignorehitpause = 1

[State 50, Manual_Vel]
type = VelAdd
triggerall = !AILevel && time
trigger1 = Command = "holdfwd" || Command = "holdback"
x = IFElse(Command = "holdback", -0.2, 0.2) * Const(Size.xscale)

[State 50, Speed_Limiter]
type = VelSet
triggerall = time && sysvar(1) = 1
trigger1 = vel x > (2 * const(velocity.jump.fwd.x) * Const(Size.xScale))
x = (2 * const(velocity.jump.fwd.x) * Const(Size.xScale))

[State 50, Speed_Limiter]
type = VelSet
triggerall = time && sysvar(1) = -1
trigger1 = vel x < (1.8 *const(velocity.jump.back.x) * Const(Size.xScale))
x = (1.8 *const(velocity.jump.back.x) * Const(Size.xScale))

