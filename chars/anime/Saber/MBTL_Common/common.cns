; Jump Start
[Statedef 40]
type    = S
physics = S
anim = 40
ctrl = 0
sprpriority = 1
facep2 = 1

; var(10) for AI -3=Jump Fwd -2=Neuture Jump -1=Jump Back
[State 40,AI]
type = varset
triggerall = AILevel
trigger1 = time = 0
trigger1 = !(var(10) = [-3,-1])
sysvar(1) = 0

[State 40,AI]
type = varset
triggerall = AILevel
trigger1 = time = 0
trigger1 = (var(10) = [-3,-1])
sysvar(1) = Abs(var(10)) - 2


;-------- -1 = normal jump, -3=super jump--------
[State 40,AI]
type = varset
triggerall = AILevel
trigger1 = time = 0
trigger1 = !(var(9) = -1 || var(9) = -3)
var(9) = 1

[State 40,AI]
type = varset
triggerall = AILevel
trigger1 = time = 0
trigger1 = (var(9) = -1 || var(9) = -3)
var(9) = Abs(var(9))


[State 40, Human]
type = VarSet
triggerall = !AILevel
trigger1 = Time = 0
sysvar(1) = 0

[State 40, Human]
type = VarSet
triggerall = !AILevel
trigger1 = command = "holdfwd"
sysvar(1) = 1

[State 40, Human]
type = VarSet
triggerall = !AILevel
trigger1 = command = "holdback"
sysvar(1) = -1

[State 40, Human]
type = varset
triggerall = !AILevel
trigger1 = !time
var(9) = ifelse(command = "highjump", 3, 1)

[State 40, Apply]
type = VelSet
trigger1 = AnimTime = 0
x = ifelse(sysvar(1)=0, const(velocity.jump.neu.x), ifelse(sysvar(1)=1, const(velocity.jump.fwd.x) * IFElse(var(9)=3,2,1), const(velocity.jump.back.x) * IFElse(var(9)=3,1.8,1))) * Const(size.xscale)
y = const(velocity.jump.y) * IfElse(var(9)=3 && sysvar(1)=0, 1.15, 1) * Const(size.yscale)

[State 40, Apply]
type = VelSet
trigger1 = AnimTime = 0
trigger1 = prevstateno = 100 ;RUN_FWD
trigger1 = sysvar(1) = 1
x = const(velocity.runjump.fwd.x) * Const(Size.xScale)


[State 40, ForceFwd]
type = VarSet
triggerall = var(1) < 10000 && var(9) = 3
trigger1 = AnimTime = 0
sysvar(1) = 1

[State 40, ForceFwd]
type = VelSet
triggerall = var(1) < 10000 && var(9) = 3
trigger1 = AnimTime = 0
x = 2 * const(velocity.jump.fwd.x) * Const(Size.xScale)

[State 40, AirJump_Dust]
type = Helper
triggerall = PrevStateNo >= 200
trigger1 = AnimTime = 0
helpertype = Normal
name = "AirJump_Dust"
ID = 7017
StateNo = 7017
postype = p1
pos = 0 * Const(Size.xScale), Const(Size.mid.pos.y) * Const(Size.yScale)
facing = 1
keyctrl = 0
ownpal = 1
;remappal = 1,1
supermovetime = 4294967295
pausemovetime = 4294967295
size.xscale = Const(Size.xScale) * 1
size.yscale = Const(Size.yScale) * 1
;size.proj.doscale = 0
size.shadowoffset = -30 * sysvar(1);Degree
ignorehitpause = 1

[State 40, SND-Step]
type = PlaySnd
trigger1 = AnimTime = 0
value = 0,105
channel = 1

[State 40, 6]
type = ChangeState
trigger1 = AnimTime = 0
value = 50
ctrl = 1

