guard.sparkno = S30040
sparkxy = -5, -50               ;ヒットエフェクトを表示する場所（相手の座標で）
hitsound = s55,4                        ;ヒットした時の音（ｓ*,*にすると自分の音を使う）
guardsound = s66,0                ;ガードした時の音（同上）
